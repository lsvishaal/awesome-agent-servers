{
    "author": "vishaallsv@gmail.com",
    "name": "competitor-analysis-agent",
    "description": "Competitive intelligence agent using Firecrawl (search/crawl/map) + ReasoningTools to generate a structured competitor analysis report.",
    "version": "1.0.0",
    "recreate_keys": false,
    
    "skills": [],
    
    "capabilities": {
    },
    

    "agent_trust": "low",
    "debug_mode": true,
    "debug_level": 2,
    
    "deployment": {
        "url": "http://0.0.0.0:3773",
        "expose": true,
        "protocol_version": "1.0.0",
        "proxy_urls": ["0.0.0.0"],
        "cors_origins": ["*"]
    },
    "storage": {
      "type": "memory"
    },
    "scheduler": {
      "type": "memory"
    },

    
    "num_history_sessions": 5,
    "environment_variables": [
      {
        "key": "OPENROUTER_API_KEY",
        "description": "OpenRouter API key for LLM calls",
        "required": true
      },
      {
        "key": "MEM0_API_KEY",
        "description": "Mem0 API key for memory operations",
        "required": true
      }
    ]
  }
